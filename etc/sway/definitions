set $mod Mod4
set $alt Mod1
set $left h
set $down j
set $up k
set $right l

set $theme /etc/sway/themes/seoul256

set $shutdown wlogout --buttons-per-row 3
set $idle swayidle -w timeout 300 'swaymsg "output * dpms off"' resume 'swaymsg "output * dpms on"'

